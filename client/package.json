{
  "name": "taiga",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/cli": "^3.1.1",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.4",
    "@vue/cli-plugin-pwa": "^3.1.1",
    "@vue/cli-plugin-typescript": "^3.1.1",
    "@vue/cli-service": "^3.1.3",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^3.1.0",
    "body-parser": "1.18.3",
    "colors": "1.1.2",
    "cookie-parser": "1.4.3",
    "material-icons": "^0.2.3",
    "morgan": "1.8.1",
    "prettyjson": "1.2.1",
    "pug": "2.0.3",
    "register-service-worker": "^1.0.0",
    "socket.io-client": "^2.1.1",
    "socket.io-p2p": "^2.2.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-scroll": "^2.1.9",
    "vue-template-compiler": "^2.5.17",
    "vuesax": "^3.7.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/socket.io-client": "^1.4.32",
    "@types/socket.io-p2p": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "typescript": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "typescript-eslint-parser"
    },
    "rules": {
      "vue/require-v-for-key": "off",
      "vue/valid-v-for": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
